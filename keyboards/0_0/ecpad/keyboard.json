{
    "manufacturer": "Alabahuy",
    "keyboard_name": "EC 2x3",
    "maintainer": "Cipulot",
    "bootloader": "stm32-dfu",
    "build": {
        "lto": true
    },
    "qmk": {
        "locking": {
            "enabled": true,
            "resync": true
        }
    },
    "diode_direction": "ROW2COL",
    "features": {
        "bootmagic": false,
        "console": true,
        "nkro": true
    },
    "processor": "STM32F401",
    "url": "https://www.github.com/Cipulot/EC23U",
    "usb": {
        "device_version": "0.0.1",
        "pid": "0x1123",
        "shared_endpoint": {
            "keyboard": true
        },
        "vid": "0x414C"
    },
    "layouts": {
        "LAYOUT_ortho_2x3": {
            "layout": [
                {"label": "0,0", "matrix": [0, 0], "x": 0, "y": 0},
                {"label": "0,1", "matrix": [0, 1], "x": 0, "y": 1},
                {"label": "0,2", "matrix": [0, 2], "x": 0, "y": 2},
                {"label": "1,0", "matrix": [1, 0], "x": 1, "y": 0},
                {"label": "1,1", "matrix": [1, 1], "x": 1, "y": 1},
                {"label": "1,2", "matrix": [1, 2], "x": 1, "y": 2}
            ]
        }
    }
}
